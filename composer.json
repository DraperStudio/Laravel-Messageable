{
    "name": "artisanry/messageable",
    "description": "Messageable Polymorphic Eloquent Models for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-messageable",
        "Laravel Messageable"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "illuminate/database": "^5.8",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Messageable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Messageable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Messageable\\MessageableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
